<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="-1dp"
    tools:layout_editor_absoluteY="0dp">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="419dp"
        android:layout_height="43dp"
        android:background="#696A6D"
        android:minHeight="?attr/actionBarSize"
        app:contentInsetStart="0dp"
        app:title=""
        app:titleTextColor="@android:color/white"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="-3dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="133dp"
        android:layout_height="31dp"
        app:srcCompat="@drawable/logo1"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="0dp"
        android:layout_height="291dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="200dp"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintDimensionRatio="w,1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/Fondo1" />

    <Button
        android:id="@+id/button2"
        android:layout_width="230dp"
        android:layout_height="55dp"
        android:backgroundTint="#A47C36"
        android:text="RESERVA CON NOSOTROS"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="347dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="227dp"
        android:layout_height="55dp"
        android:backgroundTint="#A3A3A2"
        android:text="DESTINOS"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="402dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="227dp"
        android:layout_height="54dp"
        android:backgroundTint="#A47C36"
        android:text="consulta tu reserva"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="469dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="228dp"
        android:layout_height="55dp"
        android:backgroundTint="#233C79"
        android:text="ASESOR VIRTUAL"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="533dp" />
</androidx.constraintlayout.widget.ConstraintLayout>